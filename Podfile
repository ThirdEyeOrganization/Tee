# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Tee' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Tee
pod 'Firebase'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'SVProgressHUD'


  target 'TeeTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Firebase'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'SVProgressHUD'

  end

  target 'TeeUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'Firebase'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'SVProgressHUD'

  end

end


